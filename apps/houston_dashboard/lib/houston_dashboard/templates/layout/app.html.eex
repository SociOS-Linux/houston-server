    <%= Enum.map(get_flash(@conn), fn {flash_key, flash_message} -> %>
      <div role="alert" class="alert alert--<%= flash_key %>">
        <p><%= flash_message %></p>
      </div>
    <% end) %>

    <main role="main" class="container">
      <%= render @view_module, @view_template, assigns %>
    </main>

    <footer>
      <div>
        <p>
          <%= dgettext "layout", "Copyright © %{year} elementary, Inc.", %{year: DateTime.utc_now().year} %>
        </p>
      </div>

      <ul>
        <li><a href="https://elementary.io/press"><%= dgettext "layout", "Press" %></a></li>
        <li><a href="https://elementary.io/brand"><%= dgettext "layout", "Brand" %></a></li>
        <li><a href="https://elementary.io/oem"><%= dgettext "layout", "OEMs" %></a></li>
        <li><a href="https://elementary.io/privacy"><%= dgettext "layout", "Privacy" %></a></li>
        <li><a href="https://elementary.io/team"><%= dgettext "layout", "Team" %></a></li>
        <li><a href="https://elementary.io/open-source"><%= dgettext "layout", "Open Source" %></a></li>
      </ul>
    </footer>

    <script>window.userToken = "<%= user_token @conn %>"</script>
    <script type="text/javascript" src="https://elementary.io/scripts/main.js"></script>
    <script type="text/javascript" src="<%= Routes.static_path(@conn, "/js/app.js") %>"></script>
  </body>
</html>
